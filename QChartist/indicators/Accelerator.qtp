declare sub hideAcceleratorform

CREATE Acceleratorform as qform

    Center
    Caption = "Settings for: Accelerator"
    visible=0

    ' Put your indicator settings code here

    create Acceleratorbtnok as qbutton
        left=0
        top=0
        caption="OK"
        onclick=hideAcceleratorform
    end create

END CREATE

sub hideAcceleratorform
    Acceleratorform.visible=0
end sub
