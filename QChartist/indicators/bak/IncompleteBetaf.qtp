declare sub hideIncompleteBetafform

CREATE IncompleteBetafform as qform

    Center
    Caption = "Settings for: IncompleteBetaf"
    visible=0

    ' Put your indicator settings code here

    create IncompleteBetafbtnok as qbutton
        left=0
        top=0
        caption="OK"
        onclick=hideIncompleteBetafform
    end create

END CREATE

sub hideIncompleteBetafform
    IncompleteBetafform.visible=0
end sub
