declare sub hideIGammaform

CREATE IGammaform as qform

    Center
    Caption = "Settings for: Incomplete gamma integral"
    visible=0

    ' Put your indicator settings code here

    create IGammabtnok as qbutton
        left=0
        top=0
        caption="OK"
        onclick=hideIGammaform
    end create

END CREATE

sub hideIGammaform
    IGammaform.visible=0
end sub
