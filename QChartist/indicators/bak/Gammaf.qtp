declare sub hideGammafform

CREATE Gammafform as qform

    Center
    Caption = "Settings for: Gammaf"
    visible=0

    ' Put your indicator settings code here

    create Gammafbtnok as qbutton
        left=0
        top=0
        caption="OK"
        onclick=hideGammafform
    end create

END CREATE

sub hideGammafform
    Gammafform.visible=0
end sub
