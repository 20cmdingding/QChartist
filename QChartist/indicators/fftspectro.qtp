declare sub hidefftspectroform

CREATE fftspectroform as qform

    Center
    Caption = "Settings for: fftspectro"
    visible=0

    ' Put your indicator settings code here

    create fftspectrobtnok as qbutton
        left=0
        top=0
        caption="OK"
        onclick=hidefftspectroform
    end create

END CREATE

sub hidefftspectroform
    fftspectroform.visible=0
end sub
